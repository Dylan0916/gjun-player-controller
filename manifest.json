{
  "manifest_version": 3,
  "name": "Gjun Player Controller",
  "version": "1.0.0",
  "description": "Custom keyboard shortcuts for Gjun video player",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://fs1.abconline.com.tw/course/player/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["https://fs1.abconline.com.tw/*"]
    }
  ]
}
